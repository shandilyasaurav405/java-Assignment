// 1.Write a program to calculate sum of  first N  natural numbers.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int sum=0;
        for (int i =1; i<=N;i++){
           sum = sum+i;

        }
        System.out.println(sum);
    }
}




// 2.Write a program to calculate sum of  first N even natural numbers.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int sum=0;
        for (int i =1; i<=N;i++){
            if (i%2==0) {
                sum = sum + i;
            }

        }
        System.out.println(sum);
    }
}



// 3.Write a program to calculate sum of  first N odd natural numbers.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int sum=0;
        for (int i =1; i<=N;i++){
            if (i%2 !=0){
                sum = sum+i;
            }

        }
        System.out.println(sum);
    }
}


// 4.Write a program to calculate sum of squares of the first N natural numbers.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int sum=0;
        for (int i =1; i<=N;i++){
          sum = sum+(i*i);
            }
        System.out.println(sum);
    }
}



// 5.Write a program to calculate sum of cube of the first N natural numbers.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int sum=0;
        for (int i =1; i<=N;i++){
          sum = sum+(i*i*i);
            }
        System.out.println(sum);
    }
}




// 6.Write a program to calculate factorial of a number.( use any loop).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int fact=1;
        for (int i =1; i<=N;i++){
            fact = fact*i;
            }
        System.out.println(fact);
    }
}



