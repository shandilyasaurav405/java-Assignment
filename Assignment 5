// 1.Write a program to print your name N times on the screen.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){

            System.out.println("Saurav");

        }


    }


}


// 2.Write a program to print the N natural numbers.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){

            System.out.println(i);

        }


    }


}



// 3.Write a program to print the N natural numbers in reverse order.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =N;i>=1;i--){

            System.out.println(i);

        }


    }


}



// 4.Write a program to print the first  N odd  natural numbers.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){
                System.out.println(2*i-1);
        }
    }
}



// 5.Write a program to print the first N odd natural numbers in reverse order.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =N;i>=1;i--){
                System.out.println(2*i-1);
        }
    }
}



// 6.Write a program to print the first N even natural numbers.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){
                System.out.println(2*i);
        }
    }
}




// 7.Write a program to print the first N even natural numbers in reverse order.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =N;i>=1;i--){
                System.out.println(2*i);
        }
    }
}



// 8.Write a program to print the squares of the first N  natural numbers.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){
                System.out.println(i*i);
        }
    }
}




// 9.Write a program to print the cubes of the first N  natural numbers.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=N;i++){
                System.out.println(i*i*i);
        }
    }
}



//Write a program to print a table of N.( by iterative control statements).


import java.util.Scanner;

public class Assignment {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i =1;i<=10;i++){
                System.out.println(N +"*"+i+ "=" +N*i);
        }
    }
}



